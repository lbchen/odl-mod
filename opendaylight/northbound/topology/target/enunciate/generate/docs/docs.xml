  <rest baseAddress="http://localhost:8080/full" wadl="application.wadl">
    <resources>
      <resource name="/{containerName}" subcontext="/controller/nb/v2/topology">
        <groups>
          <group>TopologyNorthboundJAXRS</group>
        </groups>
        <operation name="GET">
          <documentation>
            <![CDATA[Retrieve the Topology]]>
          </documentation>
          <contentType type="*/*" consumable="true" produceable="false"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="false" produceable="true"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[The container for which we want to retrieve the topology]]>
          </parameter>
          <outValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="topology" />
            
            <documentation><![CDATA[A List of EdgeProps each EdgeProp represent an Edge of the grap
with the corresponding properties attached to it.]]></documentation>
          </outValue>
          <statusCode code="404">
            <![CDATA[The Container Name passed was not found]]>
          </statusCode>
        </operation>
      </resource>
      <resource name="/{containerName}/userLink" subcontext="/controller/nb/v2/topology">
        <groups>
          <group>TopologyNorthboundJAXRS</group>
        </groups>
        <operation name="GET">
          <documentation>
            <![CDATA[Retrieve the user configured links]]>
          </documentation>
          <contentType type="*/*" consumable="true" produceable="false"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="false" produceable="true"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[The container for which we want to retrieve the user links]]>
          </parameter>
          <outValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="topologyUserLinks" />
            
            <documentation><![CDATA[A List of user configured links]]></documentation>
          </outValue>
          <statusCode code="404">
            <![CDATA[The Container Name passed was not found]]>
          </statusCode>
        </operation>
        <operation name="POST">
          <documentation>
            <![CDATA[Add an User Link]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The base Container is "default".]]>
          </parameter>
          <inValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="topologyUserLinkConfig" />
            
            <documentation><![CDATA[(no documentation provided)]]></documentation>
          </inValue>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response Status code]]></documentation>
          </outValue>
          <statusCode code="201">
            <![CDATA[User Link added successfully]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The Container Name passed was not found]]>
          </statusCode>
          <statusCode code="409">
            <![CDATA[Failed to add User Link due to Conflicting Name]]>
          </statusCode>
          <statusCode code="500">
            <![CDATA[Failed to add User Link. Failure Reason included in HTTP Error response]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller services are unavailable]]>
          </statusCode>
        </operation>
      </resource>
      <resource name="/{containerName}/userLink/{name}" subcontext="/controller/nb/v2/topology">
        <groups>
          <group>TopologyNorthboundJAXRS</group>
        </groups>
        <operation name="DELETE">
          <documentation>
            <![CDATA[Delete an User Link]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The base Container is "default".]]>
          </parameter>
          <parameter name="name" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Link Configuration]]>
          </parameter>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response Status code]]></documentation>
          </outValue>
          <statusCode code="200">
            <![CDATA[Operation successful]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The Container Name or Link Configuration Name was not found]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller services are unavailable]]>
          </statusCode>
        </operation>
      </resource>
