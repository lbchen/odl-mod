  <rest baseAddress="http://localhost:8080/full" wadl="application.wadl">
    <resources>
      <resource name="/{containerName}" subcontext="/controller/nb/v2/flow">
        <groups>
          <group>FlowProgrammerNorthbound</group>
        </groups>
        <operation name="GET">
          <documentation>
            <![CDATA[Returns a list of Flows configured on the given container]]>
          </documentation>
          <contentType type="*/*" consumable="true" produceable="false"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="false" produceable="true"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The Container name for the base controller is "default".]]>
          </parameter>
          <outValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="flowConfigs" />
            
            <documentation><![CDATA[List of configured flows configured on a given container]]></documentation>
          </outValue>
          <statusCode code="200">
            <![CDATA[Operation successful]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The containerName is not found]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller Services are unavailable]]>
          </statusCode>
        </operation>
      </resource>
      <resource name="/{containerName}/{nodeType}/{nodeId}" subcontext="/controller/nb/v2/flow">
        <groups>
          <group>FlowProgrammerNorthbound</group>
        </groups>
        <operation name="GET">
          <documentation>
            <![CDATA[Returns a list of Flows configured on a Node in a given container]]>
          </documentation>
          <contentType type="*/*" consumable="true" produceable="false"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="false" produceable="true"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The Container name for the base controller is "default".]]>
          </parameter>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Type of the node being programmed]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Node Identifier]]>
          </parameter>
          <outValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="flowConfigs" />
            
            <documentation><![CDATA[List of configured flows configured on a Node in a container]]></documentation>
          </outValue>
          <statusCode code="200">
            <![CDATA[Operation successful]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The containerName or nodeId is not found]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller Services are unavailable]]>
          </statusCode>
        </operation>
      </resource>
      <resource name="/{containerName}/{nodeType}/{nodeId}/{name}" subcontext="/controller/nb/v2/flow">
        <groups>
          <group>FlowProgrammerNorthbound</group>
        </groups>
        <operation name="GET">
          <documentation>
            <![CDATA[Returns the flow configuration matching a human-readable name and nodeId
on a given Container.]]>
          </documentation>
          <contentType type="*/*" consumable="true" produceable="false"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="false" produceable="true"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The Container name for the base controller is "default".]]>
          </parameter>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Type of the node being programmed]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Node Identifier]]>
          </parameter>
          <parameter name="name" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Human-readable name for the configured flow.]]>
          </parameter>
          <outValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="flowConfig" />
            
            <documentation><![CDATA[Flow configuration matching the name and nodeId on a Container]]></documentation>
          </outValue>
          <statusCode code="200">
            <![CDATA[Operation successful]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The containerName or NodeId or Configuration name is not found]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller Services are unavailable]]>
          </statusCode>
        </operation>
        <operation name="POST">
          <documentation>
            <![CDATA[Add a flow configuration]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The Container name for the base controller is "default".]]>
          </parameter>
          <parameter name="name" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Static Flow configuration]]>
          </parameter>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Type of the node being programmed]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Node Identifier]]>
          </parameter>
          <inValue>
            <xmlElement elementSchemaId="ns0" elementNamespace="" elementName="flowConfig" />
            
            <documentation><![CDATA[(no documentation provided)]]></documentation>
          </inValue>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response Status code]]></documentation>
          </outValue>
          <statusCode code="201">
            <![CDATA[Flow Config processed successfully]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The Container Name or nodeId or configuration name is not found]]>
          </statusCode>
          <statusCode code="406">
            <![CDATA[Cannot operate on Default Container when other Containers are active]]>
          </statusCode>
          <statusCode code="409">
            <![CDATA[Failed to create Static Flow entry due to Conflicting Name]]>
          </statusCode>
          <statusCode code="500">
            <![CDATA[Failed to create Static Flow entry. Failure Reason included in HTTP Error response]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller services are unavailable]]>
          </statusCode>
        </operation>
        <operation name="DELETE">
          <documentation>
            <![CDATA[Delete a Flow configuration

DELETE /flows/{containerName}/{nodeType}/{nodeId}/{name}]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The Container name for the base controller is "default".]]>
          </parameter>
          <parameter name="name" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Static Flow configuration]]>
          </parameter>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Type of the node being programmed]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Node Identifier]]>
          </parameter>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response code]]></documentation>
          </outValue>
          <statusCode code="200">
            <![CDATA[Flow Config deleted successfully]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The Container Name or Node-id or Flow Name passed is not found]]>
          </statusCode>
          <statusCode code="406">
            <![CDATA[Cannot operate on Default Container when other Containers are active]]>
          </statusCode>
          <statusCode code="500">
            <![CDATA[Failed to delete Flow config. Failure Reason included in HTTP Error response]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller service is unavailable]]>
          </statusCode>
        </operation>
        <operation name="PUT">
          <documentation>
            <![CDATA[Toggle a Flow configuration]]>
          </documentation>
          <contentType type="*/*" consumable="false" produceable="true"/>
          <contentType type="application/xml" consumable="true" produceable="true"/>
          <contentType type="application/json" consumable="true" produceable="false"/>
          <parameter name="containerName" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Container. The Container name for the base controller is "default".]]>
          </parameter>
          <parameter name="nodeType" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Type of the node being programmed]]>
          </parameter>
          <parameter name="nodeId" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Node Identifier]]>
          </parameter>
          <parameter name="name" type="path" xmlTypeName="string" xmlTypeNamespace="http://www.w3.org/2001/XMLSchema">
            <![CDATA[Name of the Static Flow configuration]]>
          </parameter>
          <outValue>
            
            
            <documentation><![CDATA[Response as dictated by the HTTP Response code]]></documentation>
          </outValue>
          <statusCode code="200">
            <![CDATA[Flow Config deleted successfully]]>
          </statusCode>
          <statusCode code="404">
            <![CDATA[The Container Name or Node-id or Flow Name passed is not found]]>
          </statusCode>
          <statusCode code="406">
            <![CDATA[Cannot operate on Default Container when other Containers are active]]>
          </statusCode>
          <statusCode code="500">
            <![CDATA[Failed to delete Flow config. Failure Reason included in HTTP Error response]]>
          </statusCode>
          <statusCode code="503">
            <![CDATA[One or more of Controller service is unavailable]]>
          </statusCode>
        </operation>
      </resource>
